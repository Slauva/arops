{
  "name": "@slauva/arops",
  "version": "0.1.11",
  "description": "A simple and lightweight library of utilities for working with arrays",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "type": "module",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "doc": "typedoc --options typedoc.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "author": "Slauva",
  "license": "MIT",
  "keywords": [
    "array",
    "utils",
    "math",
    "boolean logic",
    "boolean"
  ],
  "homepage": "https://slauva.github.io/arops/",
  "repository": {
    "url": "git+https://github.com/Slauva/arops.git"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.6.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "commitlint": "^19.6.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.1",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typedoc": "^0.27.2",
    "typescript": "^5.7.2"
  },
  "lint-staged": {
    "{src,tests}/**/*.{js,ts}": [
      "prettier --write -u"
    ],
    "*.+(json|css|md|ya?ml)": "prettier --write -u"
  },
  "bugs": {
    "url": "https://github.com/Slauva/arops/issues"
  },
  "directories": {
    "test": "tests"
  }
}
